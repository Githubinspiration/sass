    "@chakra-ui/system": "^2.5.8",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@fontsource/inter": "^4.5.15",
    "@mapbox/rehype-prism": "^0.8.0",
    "@mdx-js/loader": "^2.3.0",
    "@next/bundle-analyzer": "^13.4.3",
    "@octokit/rest": "^19.0.9",
    "@saas-ui/command-bar": "workspace:^",
